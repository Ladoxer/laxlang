{
  "name": "laxlang",
  "displayName": "laxlang",
  "description": "Language support for LaxLang - a TypeScript-inspired language for MEAN stack developers.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "contributes": {
    "languages": [
      {
        "id": "laxlang",
        "aliases": [
          "laxlang",
          "laxlang"
        ],
        "extensions": [
          ".lax"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "laxlang",
        "scopeName": "",
        "path": "./syntaxes/laxlang.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vsce": "^2.15.0",
    "vscode": "^1.1.37"
  }
}
